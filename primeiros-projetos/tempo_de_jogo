Algoritmo "tempo_de_jogo"

Var
   x, y, duracao : inteiro

Inicio

   escreval("Hora inicial: ")
   leia(x)
   escreval("Hora final: ")
   leia(y)
   
   se y > x entao
      duracao <- y - x
   senao
      duracao <- (24 - x) + y
   fimse
   
   escreval("O JOGO DUROU", duracao, " HORA(S)")
   

   


Fimalgoritmo